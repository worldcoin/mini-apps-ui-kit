{
  "name": "mini-apps-ui-kit-monorepo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "turbo build --filter=@mini-apps-ui-kit/react",
    "dev": "turbo dev",
    "dev:sb": "turbo dev:sb",
    "lint": "turbo lint",
    "build:sb": "turbo build:sb",
    "preview:sb": "pnpm dlx http-server ./packages/mini-apps-ui-kit-react/storybook-static",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.3.0"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.12.1"
}
